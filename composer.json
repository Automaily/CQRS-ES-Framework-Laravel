{
  "name": "automaily/cqrs-es-framework-laravel",
  "description": "Laravel adapter for the SmoothPhp CQRS-ES-Framework",
  "keywords": [
    "cqrs",
    "laravel",
    "event sourcing"
  ],
  "homepage": "https://github.com/Automaily/CQRS-ES-Framework-Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Bennett",
      "email": "simon@pixelatedcrow.com",
      "homepage": "https://pixelatedcrow.com",
      "role": "Developer"
    },
    {
      "name": "Jordan Crocker",
      "email": "jordan@jcrocker.uk",
      "homepage": "https://jcrocker.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "automaily/cqrs-es-framework": "dev-master",
    "laravel/framework": "^5.8|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "SmoothPhp\\LaravelAdapter\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.x-dev"
    },
    "laravel": {
      "providers": [
        "SmoothPhp\\LaravelAdapter\\ServiceProvider"
      ]
    }
  },
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/Automaily/CQRS-ES-Framework"
      }
  ]
}
